#!/usr/bin/env bash
set -e
set -o pipefail

[[ -n "$DOCKER_USERNAME" ]] || exit 0

docker images
docker login -u="$DOCKER_USERNAME" -p="$DOCKER_PASSWORD";

docker push woost/db-migration
docker push woost/core
docker push woost/web
# docker push woost/github
# docker push woost/gitter
docker push woost/slack

[[ "$TRAVIS_BRANCH" == "master" ]] && [[ -n "$STAGING_URL" ]] || exit 0

curl $STAGING_URL 2> /dev/null
